  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run server\"",
    "dev:frontend": "vite",
    "build": "tsc && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "ts-node --esm --project tsconfig.server.json src/api/serverStart.ts",
    "server:prod": "node server.js",
    "start": "npm run build && npm run build:server && npm run server:prod"
  },
